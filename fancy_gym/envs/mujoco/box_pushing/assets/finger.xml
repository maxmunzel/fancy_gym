<mujocoinclude>
<worldbody>
    <body name="finger" pos="0 0 .15">
      <geom conaffinity="0" condim="4" contype="0" type="cylinder" size="0.01 0.15" pos="0 0 0.075" mass="1.0"/>
      <site name="finger_tip" type="box" rgba="0 0 1 1." size="0.01 0.01 0.01" pos="0 0 0.2"/>
      <joint name="finger_x_joint" type="slide" pos="0 0 0" axis="1 0 0" limited="true" range=".15 1" damping="5"/>
      <joint name="finger_y_joint" type="slide" pos="0 0 0" axis="0 1 0" limited="true" range="-.8 0.8" damping="5"/>
    </body>

</worldbody>
  <actuator>
        <motor ctrllimited="true" ctrlrange="-10 10" forcelimited="true" forcerange="-50 50" joint="finger_x_joint" name="finger_x_actuator" />
        <motor ctrllimited="true" ctrlrange="-10 10" forcelimited="true" forcerange="-50 50" joint="finger_y_joint" name="finger_y_actuator" />
  </actuator>
</mujocoinclude>
