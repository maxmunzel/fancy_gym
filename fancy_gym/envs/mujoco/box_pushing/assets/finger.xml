      <joint name="finger_x_joint" type="slide" pos="0 0 0" axis="1 0 0" limited="true" range=".20 .60" damping="7"/>
      <joint name="finger_y_joint" type="slide" pos="0 0 0" axis="0 1 0" limited="true" range="-.37 0.37" damping="7"/>
