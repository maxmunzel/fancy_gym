        <motor ctrllimited="true" ctrlrange="-10 10" forcelimited="true" forcerange="-10 10" joint="finger_x_joint" name="finger_x_actuator" />
        <motor ctrllimited="true" ctrlrange="-10 10" forcelimited="true" forcerange="-10 10" joint="finger_y_joint" name="finger_y_actuator" />
        -->
